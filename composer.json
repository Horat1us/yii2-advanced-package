{
  "name": "horat1us/yii2-advanced-package",
  "description": "Yii2 Advanced Package Template",
  "type": "project",
  "require": {
    "php": ">=7.0",
    "yiisoft/yii2": "~2.0.13.1"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Horat1us/yii2-phpunit.git"
    }
  ],
  "minimum-stability": "stable",
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "yiisoft/yii2-phpunit": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Horat1us\\Package\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Horat1us\\Package\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.0"
    }
  }
}
